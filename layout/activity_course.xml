<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".course">

    <Button
        android:id="@+id/rateCourse"
        android:layout_width="164dp"
        android:layout_height="70dp"
        android:layout_marginStart="113dp"
        android:layout_marginLeft="113dp"
        android:layout_marginTop="66dp"
        android:layout_marginBottom="81dp"
        android:text="Rate the professor"
        app:layout_constraintBottom_toTopOf="@+id/textbookinfoCourse"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/discussionCourse"
        android:layout_width="164dp"
        android:layout_height="70dp"
        android:layout_marginBottom="319dp"
        android:text="discussion"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.457"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textbookinfoCourse" />

    <Button
        android:id="@+id/textbookinfoCourse"
        android:layout_width="164dp"
        android:layout_height="70dp"
        android:layout_marginStart="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginEnd="3dp"
        android:layout_marginRight="3dp"
        android:layout_marginBottom="81dp"
        android:text="textbook info"
        app:layout_constraintBottom_toTopOf="@+id/discussionCourse"
        app:layout_constraintEnd_toEndOf="@+id/rateCourse"
        app:layout_constraintStart_toStartOf="@+id/rateCourse"
        app:layout_constraintTop_toBottomOf="@+id/rateCourse" />
</androidx.constraintlayout.widget.ConstraintLayout>